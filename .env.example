APP_ENV=development
APP_PORT=8000
LOG_LEVEL=INFO

# Database (Sync URL for Alembic)
DATABASE_URL=postgresql+psycopg2://postgres:postgres@postgres:5432/bio_bind_rank

# Object Store (MinIO / S3)
OBJECT_STORE_TYPE=minio
OBJECT_STORE_ENDPOINT=minio:9000
OBJECT_STORE_USE_SSL=false
OBJECT_STORE_REGION=us-east-1
OBJECT_STORE_VERSIONING=false

OBJECT_STORE_ACCESS_KEY=minioadmin
OBJECT_STORE_SECRET_KEY=minioadmin

OBJECT_STORE_BUCKET_RAW=raw
OBJECT_STORE_BUCKET_PROCESSED=processed
OBJECT_STORE_BUCKET_ARTIFACTS=artifacts
